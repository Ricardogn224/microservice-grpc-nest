
generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["tracing"]
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  firstName  String
  lastName String
  email    String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
